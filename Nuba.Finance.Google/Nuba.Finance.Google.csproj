<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <Version>1.0.4</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Nuba</Authors>
    <Company>Nuba (www.nuba.com.ar)</Company>
    <Description>Implementation of Two different ways to get Google Finance historical prices and current (not live) candle bars for daily and intraday prices. Replace Yahoo Finance API no longer available</Description>
    <Copyright>www.nuba.com.ar</Copyright>
    <PackageProjectUrl>https://github.com/nubasoftware/Nuba.Finance.Google</PackageProjectUrl>
    <PackageIconUrl>https://secure.gravatar.com/avatar/dfb290ab2ec061a7c7e7a731f038db20</PackageIconUrl>
    <RepositoryUrl>https://github.com/nubasoftware/Nuba.Finance.Google</RepositoryUrl>
    <PackageTags>Finance Google Yahoo Intraday Daily Prices Historical Candles Candle Candlebar</PackageTags>
    <AssemblyVersion>1.0.4.0</AssemblyVersion>
    <FileVersion>1.0.4.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="2.16.3" />
  </ItemGroup>

</Project>